<!DOCTYPE html>
<html>
<head>
  <title>ShareshoreV2</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <%= render 'layouts/navigation' %>

  
  <div class="container">
    <div class="row">
      <%= render 'layouts/messages' %>
    </div>
        
    <div class="row">
      <div class"col-xs-12">
        <%= yield %>
      </div>
    </div>
    
  </div>

  <footer>
    <div class="container">
      Martin Puttke 2016
    </div>
  </footer>
</body>
</html>
